{
  "name": "my-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build-lexicon": "node tools/build-lexicon.js > ./public/lexicon.js",
    "build": "tsc && vite build",
    "dev": "FIRESTORE_EMULATOR_HOST=\"127.0.0.1:8080\" AUTH_URL=\"http://127.0.0.1:4100\" nodemon --exec \"node src/main.js\"",
    "start": "node -r @graffiticode/tracing src/main.js",
    "coverage": "nyc npm run test",
    "lint": "eslint src/ tools/",
    "lint:fix": "eslint src/ tools/ --fix",
    "build-spec": "npx spec-md ./spec/spec.md > ./public/spec.html && cp ./spec/instructions.md ./public/instructions.md && cp ./spec/template.gc ./public/template.gc",
    "watch-spec": "npx nodemon --exec 'npx spec-md > ./public/spec.html' ./spec/spec.md"
  },
  "dependencies": {
    "@graffiticode/auth": "^2.1.2",
    "@graffiticode/basis": "^1.6.2",
    "@graffiticode/tracing": "^0.3.1",
    "cors": "^2.8.5",
    "decimal.js": "^10.5.0",
    "errorhandler": "^1.5.1",
    "express": "^4.21.2",
    "graphql-request": "^6.1.0",
    "hashids": "^2.3.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "spec-md": "^3.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
